        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Chat3Tile"
        app:layout_constraintStart_toStartOf="@+id/Chat3Tile"
        app:layout_constraintTop_toTopOf="@+id/Chat3Tile"
        app:layout_constraintVertical_bias="0.40" />

    <TextView
        android:id="@+id/ChatCropName4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:elevation="3dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Chat4Tile"
        app:layout_constraintStart_toStartOf="@+id/Chat4Tile"
        app:layout_constraintTop_toTopOf="@+id/Chat4Tile"
        app:layout_constraintVertical_bias="0.40" />

    <TextView
        android:id="@+id/ChatCropName5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:elevation="3dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Chat5Tile"
        app:layout_constraintStart_toStartOf="@+id/Chat5Tile"
        app:layout_constraintTop_toTopOf="@+id/Chat5Tile"
        app:layout_constraintVertical_bias="0.40" />

    <TextView
        android:id="@+id/ChatCropName6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:elevation="3dp"
